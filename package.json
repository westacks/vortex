{
  "name": "@westacks/vortex",
  "version": "0.0.10",
  "license": "MIT",
  "description": "Server-based routing for SPAs",
  "author": "Dmytro Morozov <puny.flash@gmail.com>",
  "homepage": "https://westacks.github.io/vortex/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/westacks/vortex.git"
  },
  "bugs": {
    "url": "https://github.com/westacks/vortex/issues"
  },
  "keywords": [
    "spa",
    "routing",
    "vortex"
  ],
  "scripts": {
    "build": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "axios": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vitepress": "^1.6.3",
    "vitepress-plugin-tabs": "^0.7.1",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "@bprogress/core": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./inertia": {
      "types": "./dist/extensions/inertia.d.ts",
      "import": "./dist/inertia.js",
      "require": "./dist/inertia.cjs"
    },
    "./bprogress": {
      "types": "./dist/extensions/bprogress.d.ts",
      "import": "./dist/bprogress.js",
      "require": "./dist/bprogress.cjs"
    }
  }
}
