{
  "name": "@westacks/vortex",
  "version": "0.0.22",
  "license": "MIT",
  "description": "Server-based routing for SPAs",
  "author": "Dmytro Morozov <puny.flash@gmail.com>",
  "homepage": "https://westacks.github.io/vortex/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/westacks/vortex.git"
  },
  "bugs": {
    "url": "https://github.com/westacks/vortex/issues"
  },
  "keywords": [
    "spa",
    "routing",
    "vortex"
  ],
  "scripts": {
    "lint": "eslint src",
    "build": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "axios": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.11",
    "@types/react": "^19.1.8",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "^1.6.3",
    "vitepress-plugin-tabs": "^0.7.1",
    "vue": "^3.5.17"
  },
  "peerDependencies": {
    "@bprogress/core": "^1.0.0",
    "svelte": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "vue": "^3.0.0",
    "solid-js": "^1.0.0",
    "react": "^19.0.0"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./inertia": {
      "types": "./dist/extensions/inertia/index.d.ts",
      "import": "./dist/extensions/inertia/index.js",
      "require": "./dist/extensions/inertia/index.cjs"
    },
    "./bprogress": {
      "types": "./dist/extensions/bprogress.d.ts",
      "import": "./dist/extensions/bprogress.js",
      "require": "./dist/extensions/bprogress.cjs"
    },
    "./svelte": {
      "types": "./dist/adapters/svelte.d.ts",
      "import": "./dist/adapters/svelte.js",
      "require": "./dist/adapters/svelte.cjs"
    },
    "./vue": {
      "types": "./dist/adapters/vue.d.ts",
      "import": "./dist/adapters/vue.js",
      "require": "./dist/adapters/vue.cjs"
    },
    "./solid-js": {
      "types": "./dist/adapters/solid-js.d.ts",
      "import": "./dist/adapters/solid-js.js",
      "require": "./dist/adapters/solid-js.cjs"
    }
  }
}
